{
  "name": "kotii",
  "version": "1.0.0",
  "description": "Kotii is a react boilerplate designed to be consistent and intuitive",
  "engines": {
    "node": ">=17.2.0",
    "yarn": ">=1.22.11",
    "npm": ">=8.19.2"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "run-p clean run-s build:api",
    "build:api": "tsc -b ./tsconfig.json",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run --parallel clean",
    "release": "lerna publish",
    "start": "cd packages/kotii-scripts && node app.js start"
  },
  "license": "ISC",
  "readmeFilename": "README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/jsdom": "20",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.30.1",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.1",
    "lerna": "^5.1.8",
    "node-sass": "^7.0.1",
    "nx": "^14.4.2",
    "prettier": "^2.7.1",
    "typescript": "^4.9.4"
  }
}
